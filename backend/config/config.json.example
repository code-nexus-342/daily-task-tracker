{
  "development": {
    "username": "your_db_username",
    "password": "your_db_password",
    "database": "daily_task_db",
    "host": "127.0.0.1",
    "dialect": "postgres",
    "dialectModule": "pg"
  },
  "test": {
    "username": "your_db_username",
    "password": "your_db_password", 
    "database": "daily_task_db_test",
    "host": "127.0.0.1",
    "dialect": "postgres",
    "dialectModule": "pg"
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    },
    "pool": {
      "max": 5,
      "min": 0,
      "acquire": 30000,
      "idle": 10000
    }
  }
}